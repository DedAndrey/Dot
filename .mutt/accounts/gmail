## IMAP
set imap_user=dedikovav@gmail.com
set imap_pass=password
set folder = imaps://dedikovav@imap.gmail.com/
set spoolfile = +INBOX

## SMTP
set smtp_url=smtp://dedikovav@smtp.gmail.com:587/
set smtp_pass=password
set realname='Дедиков Андрей'
set from=dedikovav@gmail.com
set hostname="gmail.com"
set signature="С уважением, Дедиков А.В."

## Connection options
set ssl_starttls = yes
set ssl_force_tls = yes

